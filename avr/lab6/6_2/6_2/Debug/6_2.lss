                 .include "include/one_wire.asm"		; one_wire_receive_byte, one_wire_transmit_byte, one_wire_reset
                 
                 ; Dependencies: wait.asm
                 
                 ; Routine: one_wire_receive_byte
                 ; Description:
                 ; This routine generates the necessary read
                 ; time slots to receives a byte from the wire.
                 ; return value: the received byte is returned in r24.
                 ; registers affected: r27:r26 ,r25:r24
                 ; routines called: one_wire_receive_bit
                 one_wire_receive_byte:
0000b6 e0b8          ldi r27 ,8
0000b7 27aa          clr r26
                 loop_:
0000b8 d008          rcall one_wire_receive_bit
0000b9 95a6          lsr r26
0000ba fd80          sbrc r24 ,0
0000bb e880          ldi r24 ,0x80
0000bc 2ba8          or r26 ,r24
0000bd 95ba          dec r27
0000be f7c9          brne loop_
0000bf 2f8a          mov r24 ,r26
0000c0 9508          ret
                 
                 ; Routine: one_wire_receive_bit
                 ; Description:
                 ; This routine generates a read time slot across the wire.
                 ; return value: The bit read is stored in the lsb of r24.
                 ; if 0 is read or 1 if 1 is read.
                 ; registers affected: r25:r24
                 ; routines called: wait_usec
                 one_wire_receive_bit:
0000c1 9ad4          sbi DDRA ,PA4
0000c2 98dc          cbi PORTA ,PA4 ; generate time slot
0000c3 e082          ldi r24 ,0x02
0000c4 e090          ldi r25 ,0x00
0000c5 dfa9          rcall wait_usec
0000c6 98d4          cbi DDRA ,PA4 ; release the line
0000c7 98dc          cbi PORTA ,PA4
0000c8 e08a          ldi r24 ,10 ; wait 10 ?s
0000c9 e090          ldi r25 ,0
0000ca dfa4          rcall wait_usec
0000cb 2788          clr r24 ; sample the line
0000cc 99cc          sbic PINA ,PA4
0000cd e081          ldi r24 ,1
0000ce 938f          push r24
0000cf e381          ldi r24 ,49 ; delay 49 ?s to meet the standards
0000d0 e090          ldi r25 ,0 ; for a minimum of 60 ?sec time slot
0000d1 df9d          rcall wait_usec ; and a minimum of 1 ?sec recovery time
0000d2 918f          pop r24
0000d3 9508          ret
                 
                 ; Routine: one_wire_transmit_byte
                 ; Description:
                 ; This routine transmits a byte across the wire.
                 ; parameters:
                 ; r24: the byte to be transmitted must be stored here.
                 ; return value: None.
                 ; registers affected: r27:r26 ,r25:r24
                 ; routines called: one_wire_transmit_bit
                 one_wire_transmit_byte:
0000d4 2fa8          mov r26 ,r24
0000d5 e0b8          ldi r27 ,8
                 _one_more_:
0000d6 2788          clr r24
0000d7 fda0          sbrc r26 ,0
0000d8 e081          ldi r24 ,0x01
0000d9 d004          rcall one_wire_transmit_bit
0000da 95a6          lsr r26
0000db 95ba          dec r27
0000dc f7c9          brne _one_more_
0000dd 9508          ret
                 
                 ; Routine: one_wire_transmit_bit
                 ; Description:
                 ; This routine transmits a bit across the wire.
                 ; parameters:
                 ; r24: if we want to transmit 1
                 ; then r24 should be 1, else r24 should
                 ; be cleared to transmit 0.
                 ; return value: None.
                 ; registers affected: r25:r24
                 ; routines called: wait_usec
                 one_wire_transmit_bit:
0000de 938f          push r24 ; save r24
0000df 9ad4          sbi DDRA ,PA4
0000e0 98dc          cbi PORTA ,PA4 ; generate time slot
0000e1 e082          ldi r24 ,0x02
0000e2 e090          ldi r25 ,0x00
0000e3 df8b          rcall wait_usec
0000e4 918f          pop r24 ; output bit
0000e5 fd80          sbrc r24 ,0
